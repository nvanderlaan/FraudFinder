<form id="search_edit_form" action="/searches/<%= @search.id %>/edit" method="post">
  <input type="hidden" name="_method" value="PUT">
  <!-- <label for="title">Title:</label> -->
  <input type="text" id="title" name="search[title]" placeholder="Title:" value="<%= @search.title %>">
  <br>
  <!-- <label for="description">Description:</label> -->
  <input type="text" id="description" name="search[description]" placeholder="Description:" value="<%= @search.description %>">
  <br>
  <input type="submit" id="submit_button" value="Save Info">
</form>
